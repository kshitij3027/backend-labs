# === Log Generation ===
LOG_RATE=10
LOG_FORMAT=text
OUTPUT_FILE=logs/app.log
CONSOLE_OUTPUT=true

# === Log Level Distribution ===
# Format: LEVEL:weight,LEVEL:weight (weights should sum to ~1.0)
LOG_DISTRIBUTION=INFO:0.70,WARNING:0.20,ERROR:0.05,DEBUG:0.05

# === Burst Mode ===
ENABLE_BURSTS=true
BURST_FREQUENCY=0.05
BURST_MULTIPLIER=5
BURST_DURATION=3

# === Pattern Mode ===
ENABLE_PATTERNS=true
